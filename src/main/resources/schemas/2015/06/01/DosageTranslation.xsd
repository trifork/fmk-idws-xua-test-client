<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150601="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DosageTranslationLongText.xsd"/>
	<include schemaLocation="DosageTranslationShortText.xsd"/>
	<include schemaLocation="DoseQuantity.xsd"/>
	<include schemaLocation="DosageQuantityUnitText.xsd"/>
	<element name="DosageTranslation" type="medicinecard20150601:DosageTranslationType">
		<annotation>
			<documentation xml:lang="en-GB">Dosage structure translated to text plus a calulated average daily dosage</documentation>
			<documentation xml:lang="da-DK">Dosisstrukturen oversat til tekst samt beregnet gennemsnitlig daglig dosis</documentation>
		</annotation>
	</element>
	<complexType name="DosageTranslationType">
		<sequence>
			<element name="ShortText" type="medicinecard20150601:DosageTranslationShortTextType" minOccurs="0"/>
			<element name="LongText" type="medicinecard20150601:DosageTranslationLongTextType"/>
			<sequence minOccurs="0">
				<choice>
					<element name="AverageDailyDosage" type="medicinecard20150601:DoseQuantityType"/>
					<sequence>
						<element name="MinimalAverageDailyDosage" type="medicinecard20150601:DoseQuantityType"/>
						<element name="MaximalAverageDailyDosage" type="medicinecard20150601:DoseQuantityType"/>
					</sequence>
				</choice>
			</sequence>
		</sequence>
	</complexType>
</schema>

<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E1"
		elementFormDefault="qualified"
		attributeFormDefault="unqualified">

	<import schemaLocation="importsFrom20150101.xsd" namespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"/>
	<complexType name="IncludeWithdrawnDrugMedicationsType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if the medicine card is to be returned with withdrawn drug medications.</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt medicinkortet skal returneres med seponerede ordinationer.</documentation>
		</annotation>
		<sequence>
			<element name="WithdrawnAfterDateTime" type="medicinecard20150101:WithdrawnDateTimeType"/>
		</sequence>
	</complexType>
</schema>
<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="PersonIdentifier.xsd"/>
	<include schemaLocation="MedicineCardVersion.xsd"/>
	<include schemaLocation="VersionMismatchWarning.xsd"/>
	<include schemaLocation="UnpausedDrugMedication.xsd"/>
	<element name="UnpauseDrugMedicationResponse" type="medicinecard20150101:UnpauseDrugMedicationResponseType">
		<annotation>
			<documentation xml:lang="en-GB">Outer container element for a response on a request to unpause a drug medication</documentation>
			<documentation xml:lang="da-DK">Ydre container-element for et svar på en forespørgsel der fjerner pausering af en lægemiddelordination</documentation>
		</annotation>
	</element>
	<complexType name="UnpauseDrugMedicationResponseType">
		<sequence>
			<element name="PersonIdentifier" type="medicinecard20150101:PersonIdentifierType"/>
			<element name="MedicineCardVersion" type="medicinecard20150101:MedicineCardVersionType"/>
			<element name="VersionMismatchWarning" type="medicinecard20150101:VersionMismatchWarningType" minOccurs="0"/>
			<element name="DrugMedication" type="medicinecard20150101:UnpausedDrugMedicationType" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>

<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20120601="http://www.dkma.dk/medicinecard/xml.schema/2012/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2012/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="IsDoseDispensable" type="medicinecard20120601:IsDoseDispensableType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if the drug is dose dispensable. If "true" the drug can be dose dispensed</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt lægemidlet kan dosisdispenseres. Lægemidlet må dosisdispenseres hvis feltet er sat til true</documentation>
		</annotation>
	</element>
	<simpleType name="IsDoseDispensableType">
		<restriction base="boolean"/>
	</simpleType>
</schema>

<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"
        targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"
        elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="IncludeRemovedRelations" type="medicinecard20150101:IncludeRemovedRelationsType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if removed relations should be included in the response</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt fjernede relationer skal inkluderes i svaret.</documentation>
		</annotation>
	</element>
	<simpleType name="IncludeRemovedRelationsType">
		<restriction base="boolean"/>
	</simpleType>
</schema>

<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:medicinecard20150601="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01"
        targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01"
        elementFormDefault="qualified" attributeFormDefault="unqualified">

    <simpleType name="PharmacyCommentType">
        <annotation>
            <documentation xml:lang="en-GB">A comment from the pharmacy about a pharmacy effectuation</documentation>
            <documentation xml:lang="da-DK">Apotekets kommentar til en apoteksudlevering</documentation>
        </annotation>
        <restriction base="string">
            <minLength value="1"/>
            <maxLength value="100"/>
        </restriction>
    </simpleType>
</schema>

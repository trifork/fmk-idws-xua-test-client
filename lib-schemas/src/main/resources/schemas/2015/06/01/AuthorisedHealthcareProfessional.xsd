			<element name="Name" type="medicinecard20150601:AuthorisedHealthcareProfessionalNameType"/>
			<element name="SpecialityCode" type="medicinecard20150601:SpecialityCodeType" minOccurs="0"/>
		</sequence>
	</complexType>
</schema>

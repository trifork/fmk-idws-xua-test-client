<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01"
		elementFormDefault="qualified"
		attributeFormDefault="unqualified">
	<simpleType name="HasOpenDosageDispensingPrescriptionsType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if the medicine card has any open dosage dispensing prescriptions.</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt medicinkortet har recepter med dosisdispensering.</documentation>
		</annotation>
		<restriction base="boolean"/>
	</simpleType>
</schema>
